{
  "expo": {
    "name": "intership-base-project-2025",
    "slug": "intership-base-project-2025",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SFProDisplay-Bold.ttf",
            "./assets/fonts/SFProDisplay-Medium.ttf",
            "./assets/fonts/SFProDisplay-Regular.ttf",
            "./assets/fonts/SFProDisplay-Semibold.ttf"
          ]
        }
      ]
    ]
  }
}
